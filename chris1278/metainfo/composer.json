{
	"name": "chris1278/metainfo",
	"type": "phpbb-extension",
	"description": "With this extension it is possible to make your forum SEO friendly. In the admin panel it offers some functions for controlling meta information and favicons.",
	"homepage": "https://christian-esch.de",
	"version": "1.0.2",
	"time": "2021-11-11",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Christian Esch",
			"email": "smilie-man@gmx.de",
			"homepage": "https://christian-esch-de",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=7.0.0,<9.0.0",
		"phpbb/phpbb": ">=3.2.10,<3.4.0@dev",
		"composer/installers": "~1.0.0"
	},
	"extra": {
		"display-name": "Metainformation and Favicon",
			"lang-min-ver": "1.0.1",
			"soft-require": {
			"phpbb/phpbb": ">= 3.2.10,<3.4.0@dev"
		},
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/chris1278/metainfo/main",
			"filename": "metainfo.json",
			"ssl": true
		}
	}
}

